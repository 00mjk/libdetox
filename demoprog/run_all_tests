#!/bin/bash

# get demoprog directory
export DIR=$(dirname "$0")

# the sed comand addes whitespace to the begining of a line for nicer output

echo
echo simpletest
$DIR/simpletest | sed -e's/\(.*\)/   \1/'

echo
echo mediumtest
$DIR/mediumtest | sed -e's/\(.*\)/   \1/'

echo
echo longertest
$DIR/longertest | sed -e's/\(.*\)/   \1/'

echo
