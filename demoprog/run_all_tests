$DIR/ind_jmp_test | sed -e's/\(.*\)/   \1/'

echo
echo ind_jmp_reg_test
$DIR/ind_jmp_reg_test | sed -e's/\(.*\)/   \1/'

echo
echo jcctest
$DIR/jcctest | sed -e's/\(.*\)/   \1/'

echo
echo functiontest
$DIR/functiontest | sed -e's/\(.*\)/   \1/'

echo
echo adv_functiontest
$DIR/adv_functiontest | sed -e's/\(.*\)/   \1/'

echo
echo pretranslated_functiontest
$DIR/pretranslated_functiontest | sed -e's/\(.*\)/   \1/'

echo
echo bintree
$DIR/bintree | sed -e's/\(.*\)/   \1/'

echo
echo printftest
$DIR/printftest | sed -e's/\(.*\)/   \1/'

echo
echo floattest
$DIR/floattest | sed -e's/\(.*\)/   \1/'

echo
echo ret_predict_test
$DIR/ret_predict_test | sed -e's/\(.*\)/   \1/'

echo
echo powtest
$DIR/powtest | sed -e's/\(.*\)/   \1/'

